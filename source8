wait(2)

local Player = game:GetService("Players").LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()
local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")
local Head = Character:WaitForChild("Head")
local Humanoid = Character:WaitForChild("Humanoid")
local Number = 0
local Number2 = math.pi / 2
local Height = 0
local HeightIndex = 1
local Offset = 40
local Speed = 20
local Mode = 4
local Rad = 0
local Angular = Vector3.new(0, 0, 0)
local ObjectTable = {}
local TablePosition = {}
local TablePosition2 = {}
local Connection1
local Connection2
local Connection3
coroutine.resume(coroutine.create(function()
    if _G.OwnerShip == nil then
        _G.OwnerShip = true
        game:GetService("RunService").RenderStepped:Connect(function()
            settings().Physics.PhysicsEnvironmentalThrottle = Enum.EnviromentalPhysicsThrottle.Disabled
            settings().Physics.AllowSleep = false
            setsimulationradius(math.huge, math.huge)
        end)
    end
end))
local function Delete(Object)
    for _, v in pairs(Object:GetChildren()) do
        if v:IsA("SpecialMesh") or v:IsA("Weld") then
            v:Destroy()
        end
    end
end
local function GetAccessories()
    for _, v in pairs(Humanoid:GetAccessories()) do
        local Handle = v.Handle
        Handle.CustomPhysicalProperties = PhysicalProperties.new(0, 0, 0, 0, 0)
        Handle.CanCollide = false
        Handle:BreakJoints()
        Delete(Handle)
        local BodyAngularVelocity = Instance.new("BodyAngularVelocity", Handle)
        BodyAngularVelocity.MaxTorque = Vector3.new(math.huge, math.huge, math.huge)
        BodyAngularVelocity.AngularVelocity = Vector3.new(0, 0, 0)
        local AlignPosition = Instance.new("AlignPosition", Handle)
        AlignPosition.MaxForce = 99999999999999
        AlignPosition.MaxVelocity = math.huge
        AlignPosition.RigidityEnabled = false
        AlignPosition.ApplyAtCenterOfMass = true
        AlignPosition.Responsiveness = 200
        local Attachment0 = Instance.new("Attachment", Handle)
        local Attachment1 = Instance.new("Attachment", Head)
        AlignPosition.Attachment0 = Attachment0
        AlignPosition.Attachment1 = Attachment1
        table.insert(TablePosition, Attachment1)
        table.insert(TablePosition2, BodyAngularVelocity)
    end
end
local function CreatePart()
    for _, v in pairs(TablePosition) do
        local Part = Instance.new("Part", game:GetService("Workspace"))
        Part.Anchored = true
        Part.Size = Vector3.new(1, 1, 1)
        Part.Transparency = 1
        Part.CanCollide = false
        table.insert(ObjectTable, Part)
    end
end
local function Visualizer()
    local Sound = nil
    if Character:FindFirstChildOfClass("Tool") then
        if v:IsA("Tool") then
            for _, v in pairs(Character:FindFirstChildOfClass("Tool"):GetDescendants()) do
                if v:IsA("Sound") then
                    Sound = v
                end
            end
        end
    end
    if Sound ~= nil then
        Offset = Sound.PlaybackLoudness / 35
        Speed = Sound.PlaybackLoudness / 500
        Angular = Vector3.new(0, Sound.PlaybackLoudness / 75, 0)
    end
end
local function HeightFunction()
    if HeightIndex == 1 then
        Height = Height + Speed / 100
    elseif HeightIndex == 2 then
        Height = Height - Speed / 100
    end
    if Height > 2 then
        HeightIndex = 2
    elseif Height < -1 then
        HeightIndex = 1
    end
end
local function ModeFunction()
    if Mode == 1 then
        for _, v in pairs(TablePosition) do
            v.Position = Vector3.new(math.sin(Number) * Offset, 0, math.sin(Number2) * Offset)
        end
    elseif Mode == 2 then
        for _, v in pairs(TablePosition) do
            v.Position = Vector3.new(Offset, Height, Offset)
        end
    elseif Mode == 3 then
        for _, v in pairs(TablePosition) do
            v.Position = Vector3.new(math.sin(Number) * Offset, Height, math.sin(Number2) * Offset)
        end
    elseif Mode == 4 then
        for i, v in pairs(TablePosition) do
            v.Position = Vector3.new(HumanoidRootPart.CFrame:ToObjectSpace(CFrame.new(ObjectTable[i].Position)).X, HumanoidRootPart.CFrame:ToObjectSpace(CFrame.new(ObjectTable[i].Position)).Y, HumanoidRootPart.CFrame:ToObjectSpace(CFrame.new(ObjectTable[i].Position)).Z);
        end
    elseif Mode == 5 then
        for i, v in pairs(TablePosition) do
            v.Position = Vector3.new((math.sin(Number)) * Offset, Height, (math.cos(Number2) - i) * Offset)
        end
    elseif Mode == 6 then
        for i, v in pairs(TablePosition) do
            v.Position = Vector3.new((math.sin(Number)) * Offset, Height, (math.tan(Number2) - i) * Offset)
        end
    elseif Mode == 7 then
        for i, v in pairs(TablePosition) do
            v.Position = Vector3.new(math.cos(Number * i) * Offset, 0, math.cos(Number2 * i) * Offset)
        end
    elseif Mode == 8 then
        for i, v in pairs(TablePosition) do
            v.Position = Vector3.new(math.sin(Number) * i * Offset, 0, math.sin(Number2) * i * Offset)
        end
    elseif Mode == 9 then
        pcall(function()
            local so = nil;
            for k, b in pairs(chr:GetChildren()) do
                if b:IsA'Tool' then
                    for h, j in pairs(b:GetDescendants()) do
                        if j:IsA'Sound' then
                            so = j;
                        end
                    end
                end
            end
            if so ~= nil then
                offset = so.PlaybackLoudness / 35;
                speed = so.PlaybackLoudness / 500;
                angular = Vector3.new(0, so.PlaybackLoudness / 75, 0);
            end
        end)
    elseif Mode == 10 then
        Offset = Height * 15
        for i, v in pairs(TablePosition) do
            v.Position = Vector3.new(HumanoidRootPart.CFrame:ToObjectSpace(CFrame.new(ObjectTable[i].Position)).X, HumanoidRootPart.CFrame:ToObjectSpace(CFrame.new(ObjectTable[i].Position)).Y, HumanoidRootPart.CFrame:ToObjectSpace(CFrame.new(ObjectTable[i].Position)).Z)
        end
    elseif Mode == 11 then
        for i, v in pairs(TablePosition) do
            v.Position = Vector3.new(HumanoidRootPart.CFrame:ToObjectSpace(CFrame.new(Player:GetMouse().Hit.Position)).X, HumanoidRootPart.CFrame:ToObjectSpace(CFrame.new(Player:GetMouse().Hit.Position)).Y, HumanoidRootPart.CFrame:ToObjectSpace(CFrame.new(Player:GetMouse().Hit.Position)).Z) + Vector3.new(HumanoidRootPart.CFrame:ToObjectSpace(CFrame.new(ObjectTable[i].Position)).X, HumanoidRootPart.CFrame:ToObjectSpace(CFrame.new(ObjectTable[i].Position)).Y, HumanoidRootPart.CFrame:ToObjectSpace(CFrame.new(ObjectTable[i].Position)).Z)
        end
    end
    for _, v in pairs(TablePosition2) do
        v.AngularVelocity = Angular
    end
end
local function PointAtHumanoidRootPart()
    for _, v in next, Character:GetChildren() do
        if v:IsA("Accessory") and v:FindFirstChild("Handle") then
            v.Handle.CFrame = CFrame.new(v.Handle.Position, HumanoidRootPart.Position)
        end
    end
end
local function Calculation()
    Number = Number + Speed / 100
    Number2 = Number2 + Speed / 100
    Rad = (Rad >= 360 and 1 or Rad + Speed)
    for i, v in pairs(ObjectTable) do
        v.CFrame = HumanoidRootPart.CFrame * CFrame.fromEulerAnglesXYZ(8, math.rad(Rad + (360 / #ObjectTable) * i + Speed), 100) * CFrame.new(0, 0.007, 5) + HumanoidRootPart.CFrame.LookVector * -3
    end
    PointAtHumanoidRootPart()
    HeightFunction()
    ModeFunction()
end
local function MessageFunction(Message)
    local Table = {}
    if Message ~= "me" and Message ~= "random" then
        for _, v in pairs(game:GetService("Players"):GetPlayers()) do
            if v.Name:lower():find(Message:lower()) then
                table.insert(Table, v)
            end
        end
    elseif Message == "me" then
        table.insert(Table, Player)
    elseif Message == "random" then
        table.insert(Table, game:GetService("Players"):GetPlayers()[math.random(1, #game:GetService("Players"):GetPlayers())])
    end
    return Table
end
local function PlayerChatted(Message)
    if Message:split(" ")[1] == "circle" then
        for _, v in pairs(TablePosition) do
            Character = MessageFunction(Message:split(" ")[2])[1].Character
            HumanoidRootPart = MessageFunction(Message:split(" ")[2])[1].Character.HumanoidRootPart
            v.Parent = MessageFunction(Message:split(" ")[2])[1].Character.HumanoidRootPart
        end
    elseif Message:split(" ")[1] == ".Speed" then
        Speed = tonumber(Message:split(" ")[2])
    elseif Message:split(" ")[1] == ".Mode" then
        Mode = tonumber(Message:split(" ")[2])
    elseif Message:split(" ")[1] == ".Offset" then
        Offset = tonumber(Message:split(" ")[2])
    elseif Message:split(" ")[1] == ".Angular" then
        Angular = Vector3.new(tonumber(Message:split(" ")[2]), tonumber(Message:split(" ")[3]), tonumber(Message:split(" ")[4]))
    end
end
local function PlayerDied()
    Connection1:Disconnect()
    Connection2:Disconnect()
end
GetAccessories()
CreatePart()
Connection1 = game:GetService("RunService").RenderStepped:Connect(Calculation)
Connection2 = Player.Chatted:Connect(PlayerChatted)
Connection3 = Humanoid.Died:Connect(PlayerDied)

Character.Humanoid.HipHeight = 2
Character.Humanoid.WalkSpeed = 128
